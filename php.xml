<application>
  <component name="MessDetector">
    <phpmd_settings>
      <MessDetectorConfiguration />
    </phpmd_settings>
  </component>
  <component name="PhpCodeSniffer">
    <phpcs_settings>
      <PhpCSConfiguration standards="MySource;PEAR;PHPCS;PSR1;PSR2;Squiz;Zend" timeout="30000" tool_path="/opt/phpcs.phar" />
    </phpcs_settings>
  </component>
</application>