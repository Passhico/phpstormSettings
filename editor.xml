<application>
  <component name="CodeInsightSettings">
    <option name="selectAutopopupSuggestionsByChars" value="true" />
    <option name="AUTO_POPUP_JAVADOC_INFO" value="true" />
    <option name="SELECT_AUTOPOPUP_SUGGESTIONS_BY_CHARS" value="true" />
    <option name="SHOW_FULL_SIGNATURES_IN_PARAMETER_INFO" value="true" />
  </component>
  <component name="DaemonCodeAnalyzerSettings">
    <option name="SHOW_METHOD_SEPARATORS" value="true" />
  </component>
  <component name="DefaultFont">
    <option name="FONT_FAMILY" value="Hack" />
    <option name="LINE_SPACING" value="1.0" />
    <option name="USE_LIGATURES" value="true" />
  </component>
  <component name="EditorSettings">
    <option name="languageBreadcrumbsMap">
      <map>
        <entry key="TypeScript" value="true" />
      </map>
    </option>
    <option name="USE_SOFT_WRAPS" value="CONSOLE" />
    <option name="SOFT_WRAP_FILE_MASKS" value="*.md; *.txt; *.rst; *.adoc" />
    <option name="SHOW_QUICK_DOC_ON_MOUSE_OVER_ELEMENT" value="true" />
    <option name="IS_RIGHT_MARGIN_SHOWN" value="false" />
    <option name="IS_FOLDING_OUTLINE_SHOWN" value="false" />
    <option name="SHOW_BREADCRUMBS" value="false" />
    <option name="IS_INDENT_GUIDES_SHOWN" value="false" />
    <option name="IS_WHEEL_FONTCHANGE_ENABLED" value="true" />
  </component>
  <component name="TodoConfiguration">
    <pattern pattern="\b(todo|fixme)\b.*" />
  </component>
</application>